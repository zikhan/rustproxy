{
    "name": "Rust Proxy",
    "dockerComposeFile": "docker-compose.yml",
    "service": "workspace",
    "workspaceFolder": "/workspace",
    "settings": {
        "terminal.integrated.shell.linux": "/bin/bash",
        "lldb.executable": "/usr/bin/lldb"
    },
    "extensions": [
        "rust-lang.rust",
        "bungcip.better-toml",
        "vadimcn.vscode-lldb",
        "ms-azuretools.vscode-docker"
    ],
    "forwardPorts": [ 8080, 8000 ],
    "remoteUser": "vscode"
}