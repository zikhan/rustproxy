{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Launch Program",
            "program": "${workspaceRoot}/target/debug/rustproxy",
            "cwd": "${workspaceRoot}",
            "sourceLanguages": ["rust"],
            "console": "internalConsole",
            "preLaunchTask": "build",
            "env": {
                "RUST_BACKTRACE": "1",
                "AWS_ACCESS_KEY_ID": "MOCK",
                "AWS_SECRET_ACCESS_KEY": "MOCK"
            }
        }
    ]
}